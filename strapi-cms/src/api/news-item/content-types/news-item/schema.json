{
  "kind": "collectionType",
  "collectionName": "news_items",
  "info": {
    "singularName": "news-item",
    "pluralName": "news-items",
    "displayName": "News Item",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "content": {
      "type": "blocks"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "event",
        "publication",
        "achievement",
        "announcement"
      ]
    },
    "eventDate": {
      "type": "datetime"
    },
    "displayDate": {
      "type": "datetime"
    },
    "registrationLink": {
      "type": "string"
    },
    "featuredImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": ["images", "files", "videos", "audios"]
    },
    "publishedAt": {
      "type": "datetime"
    },
    "isUpcoming": {
      "type": "boolean"
    }
  }
}
